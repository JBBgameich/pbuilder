"export CCACHE_DIR=\"/var/cache/pbuilder/ccache\"\n"
"export PATH=\"/usr/lib/ccache:${PATH}\"\n"
"\n"
"EXTRAPACKAGES=ccache\n"
"BINDMOUNTS=\"${CCACHE_DIR}\""
