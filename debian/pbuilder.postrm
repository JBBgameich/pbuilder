#!/bin/sh
set -e

if [ "$1" = purge ]; then
        rm -f /etc/pbuilderrc
	if [ -e /usr/share/debconf/confmodule ]; then
	    . /usr/share/debconf/confmodule
	    db_purge
	fi
fi

dpkg-maintscript-helper rm_conffile \
	/etc/bash_completion.d/pbuilder 0.215+nmu4~ pbuilder -- "$@"

#DEBHELPER#
exit 0
